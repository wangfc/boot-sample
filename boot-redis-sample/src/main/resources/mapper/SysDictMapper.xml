<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.comma.redis.mapper.SysDictMapper">


    <select id="list" resultType="com.github.comma.redis.entity.SysDict">
        select * from sys_dict
        where name =#{name}
    </select>
</mapper>